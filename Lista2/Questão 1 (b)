#include <stdio.h>

 int main(void) {
  int a;
  int b;
  int a1;
  printf("\nDigite um valor para A: ");
  scanf("%i", &a);
  printf("\nDigite um valor para B: ");
  scanf("%i", &b);
  a1 = a;
  a = b;
  b = a1;
  printf("O novo valor de A é: %i\n", a);
  printf("O novo valor de B é: %i", b); 
return 0;
}
