#include <stdio.h>

int main()
{
    float n, i = 1, maior, menor;
    while(i <= 15){
        printf("digite a altura: ");
        scanf("%f", &n);
        if (i == 1){
            maior = n;
            menor = maior;
        }
        else if(n > maior){
            maior = n;
        }
        else{
            menor = n;
        }
        i++;
    }
    printf("o maior é: %.2f \nO menor é: %.2f", maior, menor);
        


    return 0;
}
