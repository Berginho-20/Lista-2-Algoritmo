#include <stdio.h>

int main()
{
    int n, i = 1, a = 0;
    printf("digite um valor: ");
        scanf("%d", &n);
    while(i <= n){
        
        if (n % i == 0){
           a = a + 1;
      }
           i = i + 1;
    }
    if(a == 2){
        printf("é primo");
    }
    else{
        printf("não é primo");
    }
        


    return 0;
}
