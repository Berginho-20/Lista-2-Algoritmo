#include <stdio.h>

int main(void) {
  float a, b, contB;
  printf("Escreva um número qualquer\n");
  scanf("%f", &a);
  printf("Escreva um outro número qualquer\n");
  scanf("%f", &b);
  if (a > b){
    contB = b;
    b = a;
    a = contB;
    printf("%f é maior que %f", b, a);
  }
  else 
    printf("%f é maior que %f", b, a);
  
  return 0;
}
