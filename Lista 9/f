#include <stdio.h>
#define TAM1 3
#define TAM2 6

int main(void) {
  int vet1 [TAM1], vet2 [TAM1], vet3 [TAM2], k, i;
  
  for (i = 0; i < TAM1; i++){
    printf("\nvet1[%d} = ", i);
    scanf("%d", &vet1[i]);
  }
  for (i = 0; i < TAM1; i++){
    printf("\nvet2[%d} = ", i);
    scanf("%d", &vet2[i]);
  }
  k = 0;

  for (i = 0; i < TAM1; i++){
    vet3[k] = vet1[i];
    k += 2;
  }
  k = 1;

  for (i = 0; i < TAM1; i++){
    vet3[k] = vet2[i];
    k += 2;
  }

  for (i = 0; i < TAM2; i++){
    printf("\nvet3  =  %d", vet3[i]);
    
  }
  return 0;
}
